RewriteEngine On
RewriteBase /mm/

# Index endpoints
RewriteRule ^refresh-csrf$ csrf/refresh-csrf.php [L,NC]
RewriteRule ^clear-csrf$ csrf/clear-csrf.php [L,NC]
RewriteRule ^get-network$ network/get-network.php [L,NC]
RewriteRule ^get-logs$ get-logs.php [L,NC]

// Process endpoints
RewriteRule ^process/([0-9]+)/?$ process/process.php?id=$1 [L,NC]
RewriteRule ^get-order/([0-9]+)$ process/get-order.php?id=$1 [L,NC]
RewriteRule ^get-status/([0-9]+)$ process/get-status.php?id=$1 [L,NC]
RewriteRule ^get-decimals$ process/get-decimals.php [L,NC]
RewriteRule ^create-tx/([0-9]+)$ process/create-tx.php?id=$1 [L,NC]
RewriteRule ^swap-jupiter$ process/swap-jupiter.php [L,NC]

# If accessing /error then rewrite to file error.php
RewriteRule ^error$ error.php [L]
# Redirect 301 from /error.php to /error
RewriteCond %{THE_REQUEST} \s/error\.php[\s?] [NC]
RewriteRule ^error\.php$ /error [R=301,L]
